{
  "name": "@uniformdev/canvas",
  "version": "13.0.0",
  "description": "Common functionality and types for Uniform Canvas",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli/cli.mjs",
      "require": "./dist/cli/cli.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "cli": [
        "./dist/cli/cli.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "ci:verify": "run-p lint test",
    "ci:build": "tsup --minify --clean"
  },
  "devDependencies": {
    "@types/diff": "5.0.1",
    "@types/jest": "27.0.3",
    "@types/js-yaml": "4.0.4",
    "@types/lodash.isequalwith": "4.4.6",
    "@types/node": "16.7.1",
    "@types/yargs": "17.0.5",
    "@uniformdev/cli": "^13.0.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "27.3.1",
    "npm-run-all": "4.1.5",
    "pusher-js": "7.0.3",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.7",
    "tsup": "5.7.2",
    "yargs": "17.2.1"
  },
  "peerDependencies": {
    "yargs": "^17.0.1"
  },
  "dependencies": {
    "@uniformdev/optimize-common": "^13.0.0",
    "@uniformdev/optimize-tracker-common": "^13.0.0",
    "chalk": "^4.1.2",
    "diff": "^5.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "js-yaml": "^4.1.0",
    "lodash.isequalwith": "^4.4.0",
    "p-limit": "^3.1.0",
    "p-retry": "^4.6.1",
    "p-throttle": "^4.1.1"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "7ca5b6971d631f339cfad85a7c3f415b01d22248"
